#!/bin/bash

source ~/.bashrc

echo "Use cell file: {{ ONOS_CELL }} in {{ ONOS_ROOT }}/tools/test/cells/{{ ONOS_CELL }}"
cell {{ ONOS_CELL }} > /dev/null

echo
echo "ONOS Enviroment: ~/.onos_env"
echo

echo "Run ONOS"
echo "# ok clean"
echo

echo "Run ONOS WEB GUI"
echo "http://{{ ansible_default_ipv4.address }}:8181/onos/ui/index.html"
echo "Account/Password: {{ ONOS_WEB_USER }}/{{ ONOS_WEB_PASS }}"
